{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": [
        "source.addMissingImports",
        "source.fixAll.eslint",
    ],
    "[typescript]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },
    "cSpell.words": [
        "filenamify",
        "filesnap"
    ]
}